{
  "workbench.colorTheme": "Search Lights",
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.fontFamily": "'Source Code Pro for Powerline', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.rulers": [80, 120],
  "editor.wordWrapColumn": 140,
  "extensions.ignoreRecommendations": true,
  "editor.cursorBlinking": "phase",
  "editor.wordWrap": "bounded",
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.smoothScrolling": true,
  "files.insertFinalNewline": true,
  "c-cpp-flylint.flexelint.enable": false,
  "c-cpp-flylint.cppcheck.enable": true,
  "c-cpp-flylint.standard": ["c11", "gnu11", "c++17"],
  "c-cpp-flylint.clang.standard": ["c11", "gnu11", "c++17"],
  "c-cpp-flylint.includePaths": ["${workspaceRoot}"],
  "c-cpp-flylint.run": "onType",
  "c-cpp-flylint.enable": false,
  "files.associations": {
    "*.jison": "lex",
    "*.tag": "jade",
    "*.dev": "dockerfile",
    "*.in": "plaintext"
  },
  "bashBeautify.tabSize": 2,
  "workbench.iconTheme": "field-lights-monochrome",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "C_Cpp.clang_format_fallbackStyle": "LLVM",
  "git.confirmSync": false,
  "git.autofetch": true,
  "debug.allowBreakpointsEverywhere": true,
  "breadcrumbs.enabled": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "typescript.format.enable": false,
  "typescript.check.npmIsInstalled": false,
  "typescript.format.insertSpaceAfterCommaDelimiter": false,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "typescript.format.insertSpaceAfterSemicolonInForStatements": false,
  "typescript.format.insertSpaceBeforeAndAfterBinaryOperators": false,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.suggestionActions.enabled": false,
  "clang-format.language.typescript.enable": false,
  "clang-format.language.javascript.enable": false,
  "typescript.validate.enable": false,
  "javascript.validate.enable": false,
  "shellcheck.disableVersionCheck": true,
  "workbench.settings.editor": "json",
  "C_Cpp.default.cppStandard": "c++11",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cmake.configureOnOpen": true,
  "cmake.generator": "Ninja",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[vue]": {},
  "vetur.format.defaultFormatter.ts": "none",
  "vetur.format.defaultFormatter.js": "none",
  "vs-kubernetes": {
    "vs-kubernetes.knownKubeconfigs": ["/home/mio/.kube/configs/hap/config"],
    "vs-kubernetes.kubeconfig": "/home/mio/.kube/configs/hap/config"
  },
  "azure.resourceFilter": [
    "a33c6ac4-a52e-45c5-af07-b972df9bd004/09d5a0d5-7b16-4ec1-a142-f07419b6f596"
  ],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "redhat.telemetry.enabled": false,
  "yaml.schemas": {
    "file:///home/mio/.vscode/extensions/atlassian.atlascode-3.0.7/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "github-enterprise.uri": "https://git.build.inter.ikea.com",
  "markdown.preview.breaks": true,
  "editor.largeFileOptimizations": false,
  "githubPullRequests.pullBranch": "never",
  "terraform.languageServer.enable": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "[helm]": {
    "editor.formatOnSave": false
  },
  "C_Cpp.intelliSenseEngine": "disabled",
  "jira-plugin.baseUrl": "https://plingot.atlassian.net",
  "jira-plugin.username": "mio@plingot.com",
  "window.zoomLevel": 2,
  "git.openRepositoryInParentFolders": "never"
}
