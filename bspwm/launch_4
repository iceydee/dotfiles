#!/bin/bash

. ~/my/dotfiles/bspwm/startup_functions

slowKill code
slowKill termite

setDesktop IV
launch code 1
lockNode
tmux new -s main -d
launchWithRule "termite -e 'tmux a -t main'" Termite south 0.6
lockNode
launch "google-chrome --app=https://plingot.atlassian.net/jira/software/projects/WEB/boards/1" 2
lockNode
focus termite
bspc node -z right 600 0
